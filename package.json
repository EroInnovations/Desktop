{
  "name": "qel",
  "version": "1",
  "description": "Qel Medistore App",
  "main": "index.js",
  "scripts": {
    "prepare": "node icon-setup.js",
    "start": "npm run prepare && electron .",
    "test": "echo \"No test specified\" && exit 0",
    "build": "npm run prepare && electron-builder"
  },
  "keywords": [
    "elite",
    "desktop",
    "elite_robust_ontology"
  ],
  "author": "Elite",
  "license": "ISC",
  "devDependencies": {
    "@fiahfy/icns": "^0.0.7",
    "axios": "^1.11.0",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.34.3"
  },
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "build": {
    "appId": "com.elite.qel",
    "productName": "Qel",
    "files": [
      "**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/.*",
      "!**/*.md",
      "!**/test{,s}/**",
      "!**/__tests__/**"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "public/icon.png",
      "target": "nsis"
    },
    "mac": {
      "icon": "public/icon.png",
      "target": "dmg"
    },
    "linux": {
      "icon": "public/icon.png",
      "target": "AppImage"
    },
    "publish": null
  }
}